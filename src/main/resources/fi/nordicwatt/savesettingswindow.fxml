<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>

<VBox alignment="CENTER" minHeight="150.0" minWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fi.nordicwatt.controller.SaveSettingsController">
    <children>
        <TextField fx:id="presetIdField" layoutX="14.0" layoutY="15.0" />
        <Button fx:id="saveButton" layoutX="14.0" layoutY="100.0" mnemonicParsing="false" onAction="#saveButtonAction" prefHeight="30.0" prefWidth="160.0" text="Save"/>
        <Button fx:id="cancelButton" layoutX="200.0" layoutY="100.0" mnemonicParsing="false" onAction="#cancelSaveButtonAction" prefHeight="30.0" prefWidth="160.0" text="Cancel"/>
    </children>
    <padding>
        <Insets top="10.0" left="10.0" right="10.0" bottom="10.0" />
    </padding>
</VBox>