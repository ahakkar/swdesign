<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>

<VBox alignment="CENTER" minHeight="150.0" minWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fi.nordicwatt.controller.LoadSettingsController">
    <children>
        <Label text="Choose preset"/>
        <ChoiceBox fx:id="choosePresetBox" prefWidth="150" />
        <Button fx:id="loadButton" mnemonicParsing="false" onAction="#loadButtonAction" prefHeight="30.0" prefWidth="160.0" text="Load"/>
        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelLoadButtonAction" prefHeight="30.0" prefWidth="160.0" text="Cancel"/>
    </children>
    <padding>
        <Insets top="10.0" left="10.0" right="10.0" bottom="10.0" />
    </padding>
</VBox>